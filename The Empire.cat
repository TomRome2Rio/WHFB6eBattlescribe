            <selectionEntry id="98e5-f6b1-a08d-b6de" name="Rod of Power" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="false" includeChildSelections="true" includeChildForces="true" id="4f89-47a1-5388-a7d0" type="max"/>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="cb84-d2d9-8317-86db" type="max"/>
              </constraints>
              <rules>
                <rule id="2995-9d3c-1fc9-ff02" name="Rod of Power" hidden="false">
                  <description>At the end of each Magic phase, you can save up to three dice from your Power dice pool and store them in the Rod. At the beginning of the next Magic phase (yours or the enemy&apos;s), roll a dice, if the result is equal or higher to the number of Power dice stored, add them to your pool. If the result is lower than the number of dice stored, they are lost.</description>
                </rule>
              </rules>
              <categoryLinks>
                <categoryLink id="464f-e403-5416-a8d7" name="Arcane Item" hidden="false" targetId="7cfd-b676-48e1-6eec" primary="false"/>
              </categoryLinks>
              <costs>
                <cost name="pts" typeId="eaa7-6800-e651-8bea" value="45.0"/>
                <cost name=" Hull Points" typeId="fada-3781-5bbe-4625" value="0.0"/>
