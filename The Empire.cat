            <selectionEntry id="7728-4840-aa13-d7a0" name="Luckstone" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="false" includeChildSelections="true" includeChildForces="true" id="ca00-9641-9247-ad65" type="max"/>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1b07-7a7d-43ef-9b32" type="max"/>
              </constraints>
              <rules>
                <rule id="db17-764a-a850-1796" name="Luckstone" hidden="false">
                  <description>One use only. Once per battle, the bearer can re-roll one dice used to cast or dispel a spell. This can effectively cancel a Miscast result, or cause Irresistible Force or a Miscast.</description>
                </rule>
              </rules>
              <categoryLinks>
                <categoryLink id="b8c0-9a82-1de8-2f6d" name="Arcane Item" hidden="false" targetId="7cfd-b676-48e1-6eec" primary="false"/>
