        <selectionEntry id="7eaa-2a2f-9eaa-1ae7" name="Pure of Heart" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="a2b5-f371-c4a4-a7a1" type="min"/>
