      <profiles>
        <profile id="e58d-c25e-85d9-8f2a" name="Screaming Bell" hidden="false" typeId="0a0f-00cd-0261-c0ea" typeName="Model">
          <characteristics>
            <characteristic name="M" typeId="da3c-fb2b-4c5f-a22b">-</characteristic>
