    <selectionEntry id="895b-2a7b-7ba8-2fb6" name="The Giants of Albion" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="4d45-07cc-223b-b66e" type="equalTo"/>
